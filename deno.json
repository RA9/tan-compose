{
  "compilerOptions": {
    "strict": true
  },
  "tasks": {
    "build:js": "deno run -A src/index.ts dist/bundle.js",
    "build": "deno task build:js",
    "dev": "deno task build && deno run --allow-net --allow-read --watch index.html"
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@1",
    "@twind/core": "npm:@twind/core@^1.1.3",
    "@twind/preset-autoprefix": "npm:@twind/preset-autoprefix@^1.0.7",
    "@twind/preset-tailwind": "npm:@twind/preset-tailwind@^1.1.4",
    "@twind/preset-tailwind-forms": "npm:@twind/preset-tailwind-forms@^1.1.2",
    "lit": "npm:lit@^3.2.0"
  }
}
